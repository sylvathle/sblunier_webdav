<script type="text/javascript">
        gAnswer = "";  // Global answer
        // gHTTP = getHTTPObject(); // HTTP object for XMLHttpRequest

        var datael = new Array( );
        datastate = 0;

        function dataToggle() { if ( datastate == 1 ) { datastate=0;} else {datastate=1;} dataControl(datastate);}
        function dataControl(state) {
          for (var el in datael) {
            setDisplay(datael[el],state)
          }
        }

        function pause(numberMillis) {
          var now = new Date();
          var exitTime = now.getTime() + numberMillis;
          while (true) {
            now = new Date();
            if (now.getTime() > exitTime)
            return;
          }
        }

        function domDynamicContent(elid,content) {
          rng = document.createRange();
          el = document.getElementById(elid);
          rng.setStartBefore(el);
          htmlFrag = rng.createContextualFragment(content);
          while (el.hasChildNodes()) el.removeChild(el.lastChild);
            el.appendChild(htmlFrag);
        }

        function fieldContent(elid,content) {
          rng = document.createRange();
          el = document.getElementById(elid);
          rng.setStartBefore(el);
          htmlFrag = rng.createContextualFragment(content);
          while (el.hasChildNodes()) el.removeChild(el.lastChild);
            el.appendChild(htmlFrag);
        }

        function folderContent(elid,content,folder) {
          rng = document.createRange();
          el = document.getElementById(elid);
          rng.setStartBefore(el);
          htmlFrag = rng.createContextualFragment(content);
          while (el.hasChildNodes()) el.removeChild(el.lastChild);
            el.appendChild(htmlFrag);
          el = document.getElementById('nav');
          rng.setStartBefore(el);
          htmlFrag = rng.createContextualFragment(folder);
          while (el.hasChildNodes()) el.removeChild(el.lastChild);
            el.appendChild(htmlFrag);
        }

        // Retrieve contents of a page element from a URL
        function getHTTPElement(url,elid) {
          gHTTP = new XMLHttpRequest(); // HTTP object for XMLHttpRequest

          gHTTP.open("GET",url,false); // Third param means asynch=true. Always use.
          gHTTP.onreadystatechange=function() {
            if (gHTTP.readyState==4) {
              domDynamicContent(elid,gHTTP.responseText);
              gHTTP = null;
              delete gHTTP;
            }
          }
          gHTTP.send(null)
        }

        function getHTTPFolder(url,elid,folder) {
          gHTTP = new XMLHttpRequest(); // HTTP object for XMLHttpRequest

          gHTTP.open("GET",url,false); // Third param means asynch=true. Always use.
          gHTTP.onreadystatechange=function() {
            if (gHTTP.readyState==4) {
              folderContent(elid,gHTTP.responseText, folder);
              gHTTP = null;
              delete gHTTP;
            }
          }
          gHTTP.send(null)
        }

        function setDisplay(elid,i) {
          var el = document.getElementById(elid);
          if (i == 0)
            el.style.display = "none";
          else
            el.style.display = "";
        }

        function toggleDisplay(elid) {
          // Use visibility, not this
          var el = document.getElementById(elid);
          if (el.style.display=="")
            el.style.display = "none";
          else
            el.style.display = "";
        }
        function toggleVisibility(elid) {
        var el = document.getElementById(elid);
        if (el.style.visibility=="visible")
          el.style.visibility = "hidden";
        else
          el.style.visibility = "visible";
        }
