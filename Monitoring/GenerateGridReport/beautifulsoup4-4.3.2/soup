<h3 class="ui-accordion-header"><a href="#"> Analytics</a></h3>
<div class="ui-accordion-content ui-widget-content"><div style="font-size:8pt;">
<div id="basicppopId" style="display:none">
<div id="ppopId" style="display:none"></div>
<div id="ptimesId" style="display:none"></div>
</div>
<script>
         function loadPopularity() {
            $('#ppopId').html("Downloading the popularity data . . . . Click again.");
           var aj = new AjaxRender();
           aj.download('#ppopId',function(tag,main){views().renderPopularity(tag,main);},'ppop',undefined,'atlascloud.org',0);
         }
         function loadPTimes() {
           $('#ptimesId').html("Downloading the analysis timing data . . . . Click again.");
           var aj = new AjaxRender();
           aj.download('#ptimesId',function(tag,main){views().renderPtimes(tag,main);    },'ptimes',undefined,'atlascloud.org',0);
         }
        $(document).ready( function() { $('#TimingId').click(click2show);  $('#PopularityId').click(click2show); loadPopularity(); loadPTimes(); }  );
        function click2show(event) {
            var id = $(this).attr("id");
            var ppidtxt = (id == 'PopularityId') ? 'ppopId': 'ptimesId';
            var ppid = $('#' + ppidtxt);
            event.preventDefault();
            event.stopPropagation();
            var phtml = ppid.html();
            if (phtml.indexOf('Downloading') >=0 ) {
              $('#main').html(phtml);
            } else {
               ppid.attr('id',ppid.guid());
               $('#main').empty();
               ppid.appendTo('#main');
               ppid.show();
               /* restore ppopId*/
               $('#basicppopId').append("<div id=" + ppidtxt+ " style='display:none'></div>");
               if  (id == 'PopularityId') {
                  loadPopularity();
               } else {
                  loadPTimes();
               }
            }
        }
        </script>
